<div class="container">
    <h1><%= user.name %></h1>
    <% if(user.admin){%>
    <img src="/images/admin.png"/>Admin
    <% }else{ %>
    <img src="/images/pawn.png"/>Pawn
    <% } %>
    <h3><%= user.title %></h3>
    <hr>
    <h3>Contact: <%= user.email %> </h3>
    <a class="btn btn-medium btn-primary" href="/user/edit/<%= user.id%>">Edit</a>
</div>